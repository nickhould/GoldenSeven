<% if @video.errors.full_messages.any? %>
	<% @video.errors.full_messages.each do |error_message| %>
	<%= error_message  %> <br />
	<% end %>
<% end %>
<%= form_for @video do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :url %>
	<%= f.text_field :url %>
	<%= f.submit %>
<% end %>